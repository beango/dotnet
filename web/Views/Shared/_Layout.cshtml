@using web.core.Extensions
@{
    Layout = null;
} 
<!DOCTYPE html> 
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <!-- Bootstrap 样式 -->
    @Html.StyleSheet("metronic.bootstrap/css/bootstrap.min.css")

    @RenderSection("Head", false)
</head>
<body>
    @RenderBody()
    @Html.JavaScript("jquery/jquery.min.js")
    @Html.JavaScript("metronic.bootstrap/js/bootstrap.min.js")
</body>
</html>
